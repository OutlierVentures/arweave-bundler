#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/lo/projects/ov/arweave-bundler/node_modules/.pnpm/@vercel+ncc@0.38.1/node_modules/@vercel/ncc/dist/ncc/node_modules:/Users/lo/projects/ov/arweave-bundler/node_modules/.pnpm/@vercel+ncc@0.38.1/node_modules/@vercel/ncc/dist/node_modules:/Users/lo/projects/ov/arweave-bundler/node_modules/.pnpm/@vercel+ncc@0.38.1/node_modules/@vercel/ncc/node_modules:/Users/lo/projects/ov/arweave-bundler/node_modules/.pnpm/@vercel+ncc@0.38.1/node_modules/@vercel/node_modules:/Users/lo/projects/ov/arweave-bundler/node_modules/.pnpm/@vercel+ncc@0.38.1/node_modules:/Users/lo/projects/ov/arweave-bundler/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/lo/projects/ov/arweave-bundler/node_modules/.pnpm/@vercel+ncc@0.38.1/node_modules/@vercel/ncc/dist/ncc/node_modules:/Users/lo/projects/ov/arweave-bundler/node_modules/.pnpm/@vercel+ncc@0.38.1/node_modules/@vercel/ncc/dist/node_modules:/Users/lo/projects/ov/arweave-bundler/node_modules/.pnpm/@vercel+ncc@0.38.1/node_modules/@vercel/ncc/node_modules:/Users/lo/projects/ov/arweave-bundler/node_modules/.pnpm/@vercel+ncc@0.38.1/node_modules/@vercel/node_modules:/Users/lo/projects/ov/arweave-bundler/node_modules/.pnpm/@vercel+ncc@0.38.1/node_modules:/Users/lo/projects/ov/arweave-bundler/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/ncc/cli.js" "$@"
else
  exec node  "$basedir/../../dist/ncc/cli.js" "$@"
fi
